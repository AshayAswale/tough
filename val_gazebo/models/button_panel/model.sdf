<?xml version='1.0'?>
<sdf version='1.6'>
  <model name='button_panel'>
    <link name='base'>
      <pose frame=''>-0.25 0 1 0 -0 0</pose>
      <inertial>
        <mass>50</mass>
        <inertia>
          <ixx>0.166667</ixx>
          <ixy>0</ixy>
          <ixz>0</ixz>
          <iyy>0.166667</iyy>
          <iyz>0</iyz>
