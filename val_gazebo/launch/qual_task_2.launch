<launch>
  <!-- We grab the logic in empty_world.launch, changing only the name of the world to be launched -->
  <include file="$(find gazebo_ros)/launch/empty_world.launch">
    <arg name="verbose" value="true"/>
    <arg name="world_name" value="$(find val_gazebo)/worlds/qual_task_2.world"/>
  </include>

  <node name="src_door" pkg="val_gazebo" type="src_door">
  </node>
</launch>
